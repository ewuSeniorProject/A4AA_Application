<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="A4AA_Application.SurveyPages.MainSurveyPage">
    <ScrollView>
        <StackLayout VerticalOptions="CenterAndExpand">
            <Button Text="Premises Information" Clicked="OnClickedA" />
            <Button Text="Parking, Loading Zone, Bus Stops" Clicked="OnClickedB" />
            <Button Text="Exterior &amp; Entrance" Clicked="OnClickedC" />
            <Button Text="Interior" Clicked="OnClickedD" />
            <Button Text="Elevator &amp; Lifts" Clicked="OnClickedE" />
            <Button Text="Signage" Clicked="OnClickedF" />
            <Button Text="Emergency Preparedness" Clicked="OnClickedG" />
            <Button Text="Seating" Clicked="OnClickedH" />
            <Button Text="Restrooms" Clicked="OnClickedI" />
            <Button Text="Communication Technologies &amp; Customer Service" Clicked="OnClickedJ" />
        </StackLayout>
    </ScrollView>
</ContentPage>